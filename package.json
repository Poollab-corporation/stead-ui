{
  "name": "@steadhr/ui",
  "description": "Stead UI React Component Library",
  "keywords": [
    "steadhr-ui",
    "steadhr"
  ],
  "private": false,
  "author": {
    "name": "Poollab Corporation",
    "email": "contact@poollab.kr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Poollab-corporation/stead-ui.git"
  },
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "module": "./dist/index.es.js",
      "import": "./dist/index.es.js",
      "default": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "rm -rf dist && tsc && vite build",
    "add:package": "yarn add ./stead-ui-react-v0.0.1.tgz"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.14.2",
    "@vanilla-extract/vite-plugin": "^4.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-libcss": "^1.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/builder-vite": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.0.9",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0"
  },
  "packageManager": "yarn@4.1.1"
}
